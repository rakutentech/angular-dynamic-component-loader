{
  "name": "@rak/angular-dynamic-component-loader",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build_lib": "ng build --prod angular-dynamic-component-loader",
    "test": "ng test",
    "lint": "ng lint",
    "copy-license": "cp ./LICENSE ./dist/@rak/angular-dynamic-component-loader",
    "copy-readme": "cp ./README.md ./dist/@rak/angular-dynamic-component-loader",
    "copy-files": "npm run copy-license && npm run copy-readme",
    "npm_pack": "cd dist/@rak/angular-dynamic-component-loader && npm pack",
    "package": "npm run build_lib && npm run copy-files && npm run npm_pack",
    "format": "prettier -w .",
    "format:write": "prettier -w .",
    "format:check": "prettier -c .",
    "clean": "rm -rf node_modules package-lock.json && npm cache verify",
    "clean:install": "npm run clean && npm i"
  },
  "private": false,
  "dependencies": {
    "@angular/animations": "^10.1.3",
    "@angular/common": "^10.1.3",
    "@angular/compiler": "^10.1.3",
    "@angular/core": "^10.1.3",
    "@angular/forms": "^10.1.3",
    "@angular/platform-browser": "^10.1.3",
    "@angular/platform-browser-dynamic": "^10.1.3",
    "@angular/router": "^10.1.3",
    "rxjs": "~6.5.5",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1001.2",
    "@angular-devkit/build-ng-packagr": "^0.1001.2",
    "@angular/cli": "^10.1.3",
    "@angular/compiler-cli": "^10.1.3",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0-next.1",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~3.3.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^10.1.0",
    "prettier": "2.2.1",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~3.9.7"
  },
  "description": "This library exposes a dynamic loader for angular components.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rakutentech/angular-dynamic-component-loader.git"
  },
  "bugs": {
    "url": "https://github.com/rakutentech/angular-dynamic-component-loader/issues"
  },
  "homepage": "https://github.com/rakutentech/angular-dynamic-component-loader#readme"
}
